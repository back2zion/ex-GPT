# 데이터베이스 설정
DATABASE_URL=postgresql://exgpt_user:mySecurePassword123@postgres:5432/exgpt_permissions
POSTGRES_PASSWORD=mySecurePassword123

# Redis 설정
REDIS_URL=redis://redis:6379/0

# Qdrant 설정
QDRANT_HOST=qdrant
QDRANT_PORT=6333

# JWT 설정
JWT_SECRET_KEY=your-very-secure-secret-key-change-in-production-abc123def456
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=480

# 로깅 설정
LOG_LEVEL=INFO
LOG_FILE=/app/logs/exgpt-auth.log

# 환경 설정
ENVIRONMENT=development
DEBUG=true

# 파일 업로드 설정
MAX_UPLOAD_SIZE=10485760
UPLOAD_PATH=/app/uploads

# 캐시 설정
CACHE_TTL=300

# 모니터링 설정
PROMETHEUS_ENABLED=true
METRICS_PATH=/metrics
